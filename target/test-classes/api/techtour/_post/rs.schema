{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "timestamp",
    "status",
    "error",
    "message",
    "path"
  ],
  "properties": {
    "timestamp": {
      "$id": "#/properties/timestamp",
      "type": "string",
      "title": "The Timestamp Schema",
      "default": "",
      "examples": [
        "2019-01-08T12:55:13.503+0000"
      ],
      "pattern": "^(.*)$"
    },
    "status": {
      "$id": "#/properties/status",
      "type": "integer",
      "title": "The Status Schema",
      "default": 0,
      "examples": [
        401
      ]
    },
    "error": {
      "$id": "#/properties/error",
      "type": "string",
      "title": "The Error Schema",
      "default": "",
      "examples": [
        "Unauthorized"
      ],
      "pattern": "^(.*)$"
    },
    "message": {
      "$id": "#/properties/message",
      "type": "string",
      "title": "The Message Schema",
      "default": "",
      "examples": [
        "Authentication failed"
      ],
      "pattern": "^(.*)$"
    },
    "path": {
      "$id": "#/properties/path",
      "type": "string",
      "title": "The Path Schema",
      "default": "",
      "examples": [
        "/bo/api/authentication"
      ],
      "pattern": "^(.*)$"
    }
  }
}